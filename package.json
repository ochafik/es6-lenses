{
  "name": "es6-lenses",
  "version": "0.1.6",
  "description": "Functional lenses for ES6",
  "main": "./build/index.umd.js",
  "jsnext:main": "./build/index.es2015.js",
  "types": "./index.d.ts",
  "scripts": {
    "start": "concurrently --kill-others \"npm run rollup:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "rollup": "rollup -c",
    "rollup:w": "rollup -c -w",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "npm run rollup",
    "test": "npm run tsc && mocha --reporter spec build/test",
    "test:w": "concurrently \"npm run tsc:w\" \"mocha --watch --reporter spec build/test\""
  },
  "author": {
    "name" : "Olivier Chafik",
    "email" : "olivier.chafik@gmail.com",
    "url" : "https://ochafik.com/"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^2.5.0",
    "typescript": "^2.0.3"
  }
}
